        StringBuilder stringBuilder = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; StringBuilder();
        &lt;span class=&quot;code-keyword&quot;&gt;try&lt;/span&gt; (CSVPrinter csvPrinter = &lt;span class=&quot;code-keyword&quot;&gt;new&lt;/span&gt; CSVPrinter(stringBuilder, CSVFormat.EXCEL);
             CSVParser csvParser = CSVParser.parse(in, CSVFormat.Builder.create().setDelimiter(&lt;span class=&quot;code-quote&quot;&gt;&quot;||&quot;&lt;/span&gt;).build())) {
            &lt;span class=&quot;code-keyword&quot;&gt;for&lt;/span&gt; (CSVRecord csvRecord : csvParser) {
                &lt;span class=&quot;code-keyword&quot;&gt;for&lt;/span&gt; (&lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt; i = 0; i &amp;lt; csvRecord.size(); i++) {
